{% if errorGeneral %}
	<div>
		{{ errorGeneral }}
	</div>
{% endif %}

{% if reporte %}
	<h5>Ingreso realizado</h5>
	<table class="table table-bordered table-condensed table-striped small">
		<tr>
			<th class="col-md-4">Archivo</th>
			<td>
				{{ reporte.archivo }}
			</td>
		</tr>
		<tr>
			<th>Total Registros</th>
			<td>{{ reporte.total }}</td>
		</tr>
		<tr>
			<th>Total Errores</th>
			<td>{{ reporte.errores }}</td>
		</tr>
		<tr>
			<th>Tiempo de Ejecuci√≥n</th>
			<td>{{ reporte.tiempo_ejecucion|number_format(2, '.', ',') }} minutos</td>
		</tr>
	</table>
{% endif %}
