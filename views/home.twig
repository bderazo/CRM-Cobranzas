{% extends layoutApp %}
{% block content %}

						<h4 class="">USUARIO</h4>
                        <h3 ></i>&nbsp;{{ user.nombres }}</h3><br/>
                        <h4 class="">GESTION</h4>
                        <h3 >{{ ds.gestiones }}</h3>
                        <h4 class="">CLIENTE</h4>
                        <h3 >{{ ds.clientes }}</h3>
					

<div class="col-lg-4">
        <div class="card overflow-hidden">
            <div class="card-body">
                <div class="d-flex">
                    <div class="mt-2">
                        <h4 class="">PRIMERA GESTIÓN</h4>
                        <h3 class="mb-0 number-font"><i class="fa fa-clock-o" aria-hidden="true"></i>&nbsp;{{ hora_primera_gestion }}</h3><br/>
                        <h4 class="">ÚLTIMA GESTIÓN</h4>
                        <h3 class="mb-0 number-font"><i class="fa fa-clock-o" aria-hidden="true"></i>&nbsp;{{ hora_ultima_gestion }}</h3>
					</div>

                    <div class="ms-auto" style="font-size: 80px; color: #46B476;">
                    </div>
                </div>
            </div>
        </div>
    </div>
			
				<div class="col-lg-2">
						<div class="card-container">
								{% for uc in ultimas_cargas %}
								<div class="card">
									<div class="card-header">
									Acciones:   {{ uc.tipo }}
									</div>
									<div class="card-body">
									<p>Registros: {{ uc.total_registros }}</p>
									<p>Errores: {{ uc.total_errores }}</p>
									<p>Observaciones: {{ uc.observaciones }}</p>
									<p>Fecha: {{ uc.fecha_ingreso }}</p>
									<p>Usuario: {{ uc.usuario }}</p>
									</div>
								</div>
								{% endfor %}
						</div>
	
				</div>
				<div class="col-lg-2">
        </div>
				

	<!-- INTERNAL  CHART JS-->
	<script src="{{ root }}/template_ok/plugins/chart/Chart.bundle.js"></script>
	<script src="{{ root }}/template_ok/plugins/chart/utils.js"></script>

	<!-- INTERNAL SELECT2 JS -->
	<script src="{{ root }}/template_ok/plugins/select2/select2.full.min.js"></script>

	<!-- INTERNAL DATA TABLES JS -->
	<script src="{{ root }}/template_ok/plugins/datatable/js/jquery.dataTables.min.js"></script>
	<script src="{{ root }}/template_ok/plugins/datatable/js/dataTables.bootstrap5.js"></script>
	<script src="{{ root }}/template_ok/plugins/datatable/dataTables.responsive.min.js"></script>

	<!-- INTERNAL FLOT JS -->
	<script src="{{ root }}/template_ok/plugins/flot/jquery.flot.js"></script>
	<script src="{{ root }}/template_ok/plugins/flot/jquery.flot.fillbetween.js"></script>
	<script src="{{ root }}/template_ok/plugins/flot/chart.flot.sampledata.js"></script>
	<script src="{{ root }}/template_ok/plugins/flot/dashboard.sampledata.js"></script>

	<!-- INTERNAL VECTOR JS -->
	<script src="{{ root }}/template_ok/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
	<script src="{{ root }}/template_ok/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>

	<!-- INTERNAL INDEX1 JS -->
	<script src="{{ root }}/template_ok/js/index1.js"></script>

{% endblock %}