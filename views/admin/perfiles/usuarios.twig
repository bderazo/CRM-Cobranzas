<table class="table table-condensed table-striped small">
	<tr>
		<th>Username</th>
		<th>Nombre</th>
		<th>Email</th>
	</tr>
	{% for u in usuarios %}
		<tr>
			<td>
				<a href="{{ root }}/admin/usuarios/editar?id={{ u.id }}" target="_blank">{{ u.username }}</a>
			</td>
			<td>{{ u.nombreCompleto() }}</td>
			<td>{{ u.email }}</td>
		</tr>
	{% endfor %}
</table>