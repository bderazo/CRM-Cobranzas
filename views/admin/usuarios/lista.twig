<div class="table-responsive">
	<table class="table border table-striped table-bordered table-sm mb-0">
	<thead>
	<tr>
		<th>Username</th>
		<th>Nombre Completo</th>
		<th>Activo</th>
		<th>Canal</th>
		<th>Plaza</th>
		<th>Campa√±a</th>
		<th>Identificador</th>
		<th>Fecha creacion</th>
	</tr>
	</thead>
	<tbody>
	{% for l in lista %}
		<tr>
			<td>
				<a href="usuarios/editar?id={{ l.id }}">{{ l.username }}</a>
			</td>
			<td>{{ l.apellidos }}&nbsp;{{ l.nombres }}</td>
			<td>
				{% if l.activo == 1 %}
					SI
				{% else %}
					NO
				{% endif %}
			</td>
			<td>{{ l.canal }}</td>
			<td>{{ l.plaza }}</td>
			<td>{{ l.campana }}</td>
			<td>{{ l.identificador }}</td>
			<td>{{ l.fecha_creacion|date("Y-m-d") }}</td>
		</tr>
	{% endfor %}
	</tbody>
</table>
</div>

<br/>
{{ pag|raw }}




