						cuota_mensual = parseFloat(self.aplicativo_diners_tarjeta_discover.total_financiamiento_total) / parseInt(self.aplicativo_diners_tarjeta_discover.plazo_financiamiento);
					}
					self.aplicativo_diners_tarjeta_discover.valor_cuota_mensual = cuota_mensual.toFixed(2);

					//ALERTAS
					if(parseFloat(self.aplicativo_diners_tarjeta_discover.abono_total) < parseFloat(self.aplicativo_diners_tarjeta_discover.interes_facturado)){
						$("#alerta_abono_discover").show();
					}else{
						$("#alerta_abono_discover").hide();
					}
				},

				calcularValoresMastercard: function () {
					var self = this;

					//ABONO TOTAL
					if(parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_efectivo_sistema) > 0){
						var abono_total_diners = parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_efectivo_sistema) + parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_negociador);
					}else{
						var abono_total_diners = parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_negociador);
					}
					self.aplicativo_diners_tarjeta_mastercard.abono_total = abono_total_diners.toFixed(2);

					//SALDOS FACTURADOS DESPUÉS DE ABONO
					var saldo_90_facturado = parseFloat(self.aplicativo_diners_tarjeta_mastercard.saldo_90_facturado);
					var saldo_60_facturado = parseFloat(self.aplicativo_diners_tarjeta_mastercard.saldo_60_facturado);
					var saldo_30_facturado = parseFloat(self.aplicativo_diners_tarjeta_mastercard.saldo_30_facturado);
					var saldo_actual_facturado = parseFloat(self.aplicativo_diners_tarjeta_mastercard.saldo_actual_facturado);
					var abono_total = parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_total);
					var saldo_pasa = 0;
					var saldo_90_facturado_despues_abono = saldo_90_facturado - abono_total;
					if(saldo_90_facturado_despues_abono > 0){
						self.aplicativo_diners_tarjeta_mastercard.saldo_90_facturado_despues_abono = saldo_90_facturado_despues_abono.toFixed(2);
						saldo_pasa = 0;
					}else{
						self.aplicativo_diners_tarjeta_mastercard.saldo_90_facturado_despues_abono = 0.00;
						saldo_pasa = saldo_90_facturado_despues_abono * (-1);
					}
					var saldo_60_facturado_despues_abono = saldo_60_facturado - saldo_pasa;
					if(saldo_60_facturado_despues_abono > 0){
						self.aplicativo_diners_tarjeta_mastercard.saldo_60_facturado_despues_abono = saldo_60_facturado_despues_abono.toFixed(2);
						saldo_pasa = 0;
					}else{
						self.aplicativo_diners_tarjeta_mastercard.saldo_60_facturado_despues_abono = 0.00;
						saldo_pasa = saldo_60_facturado_despues_abono * (-1);
					}
					var saldo_30_facturado_despues_abono = saldo_30_facturado - saldo_pasa;
					if(saldo_30_facturado_despues_abono > 0){
						self.aplicativo_diners_tarjeta_mastercard.saldo_30_facturado_despues_abono = saldo_30_facturado_despues_abono.toFixed(2);
						saldo_pasa = 0;
					}else{
						self.aplicativo_diners_tarjeta_mastercard.saldo_30_facturado_despues_abono = 0.00;
						saldo_pasa = saldo_30_facturado_despues_abono * (-1);
					}
					var saldo_actual_facturado_despues_abono = saldo_actual_facturado - saldo_pasa;
					if(saldo_actual_facturado_despues_abono > 0){
						self.aplicativo_diners_tarjeta_mastercard.saldo_actual_facturado_despues_abono = saldo_actual_facturado_despues_abono.toFixed(2);
					}else{
						self.aplicativo_diners_tarjeta_mastercard.saldo_actual_facturado_despues_abono = 0.00;
					}

					//VALOR A TIPO DE FINANCIAMIENTO
					if(saldo_90_facturado_despues_abono > 0){
						self.aplicativo_diners_tarjeta_mastercard.tipo_financiamiento = 'REESTRUCTURACIÓN';
					}else{
						if((saldo_60_facturado_despues_abono > 0) || (saldo_30_facturado_despues_abono > 0)){
							self.aplicativo_diners_tarjeta_mastercard.tipo_financiamiento = 'REFINANCIACIÓN';
						}else{
							self.aplicativo_diners_tarjeta_mastercard.tipo_financiamiento = 'NOVACIÓN';
						}
					}

					//VALOR A FINANCIAR
					var valor_financiar_diners = parseFloat(self.aplicativo_diners_tarjeta_mastercard.deuda_actual) + parseFloat(self.aplicativo_diners_tarjeta_mastercard.total_precancelacion_diferidos) +
						parseFloat(self.aplicativo_diners_tarjeta_mastercard.interes_facturar) + parseFloat(self.aplicativo_diners_tarjeta_mastercard.corrientes_facturar) +
						parseFloat(self.aplicativo_diners_tarjeta_mastercard.gastos_cobranza) + parseFloat(self.aplicativo_diners_tarjeta_mastercard.valor_otras_tarjetas) -
						parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_total);
					self.aplicativo_diners_tarjeta_mastercard.valor_financiar = valor_financiar_diners.toFixed(2);

					//TOTAL INTERES
					var porcentaje_interes_arr = [];
					$.each(self.aplicativo_diners_porcentaje_interes, function (key, value) {
						porcentaje_interes_arr[value.meses_plazo] = value.interes;
					});
					var porcentaje_interes = 0.00;
					var meses_plazo = parseInt(self.aplicativo_diners_tarjeta_mastercard.plazo_financiamiento) + parseInt(self.aplicativo_diners_tarjeta_mastercard.numero_meses_gracia);
					if (typeof (porcentaje_interes_arr[meses_plazo]) != "undefined" && porcentaje_interes_arr[meses_plazo] !== null) {
						porcentaje_interes = porcentaje_interes_arr[meses_plazo];
					}
					var total_interes = parseFloat(self.aplicativo_diners_tarjeta_mastercard.valor_financiar) * (porcentaje_interes / 100);
					self.aplicativo_diners_tarjeta_mastercard.total_intereses = total_interes.toFixed(2);

					//TOTAL FINANCIAMIENTO
					var total_financiamiento = parseFloat(self.aplicativo_diners_tarjeta_mastercard.valor_financiar) + parseFloat(self.aplicativo_diners_tarjeta_mastercard.total_intereses);
					self.aplicativo_diners_tarjeta_mastercard.total_financiamiento_total = total_financiamiento.toFixed(2);

					//VALOR CUOTA MENSUAL
					var cuota_mensual = 0;
					if (parseInt(self.aplicativo_diners_tarjeta_mastercard.plazo_financiamiento) > 0) {
						cuota_mensual = parseFloat(self.aplicativo_diners_tarjeta_mastercard.total_financiamiento_total) / parseInt(self.aplicativo_diners_tarjeta_mastercard.plazo_financiamiento);
					}
					self.aplicativo_diners_tarjeta_mastercard.valor_cuota_mensual = cuota_mensual.toFixed(2);

					//ALERTAS
					if(parseFloat(self.aplicativo_diners_tarjeta_mastercard.abono_total) < parseFloat(self.aplicativo_diners_tarjeta_mastercard.interes_facturado)){
						$("#alerta_abono_mastercard").show();
					}else{
						$("#alerta_abono_mastercard").hide();
					}
				},

				guardarAplicativoDiners: function () {
					var self = this;
					var data = {
						model: self.model,
						aplicativo_diners: self.aplicativo_diners,
						aplicativo_diners_tarjeta_diners: self.aplicativo_diners_tarjeta_diners,
						aplicativo_diners_tarjeta_discover: self.aplicativo_diners_tarjeta_discover,
						aplicativo_diners_tarjeta_mastercard: self.aplicativo_diners_tarjeta_mastercard,
						aplicativo_diners_tarjeta_interdin: self.aplicativo_diners_tarjeta_interdin,
					};
					$.post('{{ root }}/producto/guardarAplicativoDiners', data, function (res) {
						alert("APLICATIVO DINERS GUARDADO");
					})
				}
			},
			mounted: function () {
				var self = this;
				$("#alerta_abono_diners").hide();
				$("#alerta_abono_interdin").hide();
				$("#alerta_abono_discover").hide();
				$("#alerta_abono_mastercard").hide();
				$('#form-edit').validate({
					onfocusout: false,
					onkeyup: false,
					onclick: false,
					submitHandler: function (form) {
						var data = {
							model: self.model,
							telefono: self.telefono,
							del_telefono: self.del_telefono,
							email: self.email,
							del_email: self.del_email,
						};
						$('#json').val(JSON.stringify(data));
						$("#btnGuardar").prop("disabled", true);
						form.submit();
						return false;
					}
				});
			}
		});
	</script>
	<style>
		table, th, td {
			border: 1px solid;
		}

		table {
			width: 100%;
			border-collapse: collapse;
		}
	</style>
{% endblock %}