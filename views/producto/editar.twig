						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.dias_mora">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Número de cuotas</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.numero_cuotas">
