								<option value="OFRECIMIENTO AL CORTE">OFRECIMIENTO AL CORTE</option>
								<option value="REFINANCIA">REFINANCIA</option>
								<option value="SIN ARREGLO">SIN ARREGLO</option>
							</select>
						</div>

						<label for="inputName" class="col-md-2 form-label">NIVEL2</label>
						<div class="col-md-4">
							<select class="form-control form-select select2 required" v-model="model.tipo_perfil" placeholder="Seleccione una opción">
								<option value="COMPAÑERO DE TRABAJO M">COMPAÑERO DE TRABAJO M</option>
								<option value="EQUIVOCADO-DESCONOCEN NUM M">EQUIVOCADO-DESCONOCEN NUM M</option>
								<option value="CONTESTADORA M">CONTESTADORA M</option>
								<option value="EQUIVOCADO / OTORGAN NÚMERO M">EQUIVOCADO / OTORGAN NÚMERO M</option>
								<option value="TELÉFONO CON AVERIA M">TELÉFONO CON AVERIA M</option>
								<option value="EMPLEADA M">EMPLEADA M</option>
								<option value="FALLECIMIENTO M">FALLECIMIENTO M</option>
								<option value="SUGERENCIA MANTENIMIENTO M">SUGERENCIA MANTENIMIENTO M</option>
								<option value="VIAJE M">VIAJE M</option>
