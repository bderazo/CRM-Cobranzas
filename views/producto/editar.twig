				aplicativo_diners_tarjeta_interdin: {{ aplicativo_diners_tarjeta_interdin|raw }},
				aplicativo_diners_porcentaje_interes: {{ aplicativo_diners_porcentaje_interes|raw }},
			},
			watch: {},
			computed: {},
			methods: {
				calcularValoresDiners: function () {
					var self = this;

					//ABONO TOTAL
					var abono_total_diners = parseFloat(self.aplicativo_diners_tarjeta_diners.abono_efectivo_sistema) + parseFloat(self.aplicativo_diners_tarjeta_diners.abono_negociador);
					self.aplicativo_diners_tarjeta_diners.abono_total = abono_total_diners.toFixed(2);

					//VALOR A FINANCIAR
