{% extends 'layout.twig' %}
{% block content %}
	<div id="app">
		<div class="col-md-12 col-xl-12">
			<form method="POST" action="{{ root }}/institucion/guardar" id="form-edit-cliente" class="form-horizontal" autocomplete="off" enctype="multipart/form-data">
				<div class="card">
					<div class="card-header">
						<h4 class="card-title">CLIENTE</h4>
					</div>
					<div class="card-body">
						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Apellidos *</label>
							<div class="col-md-4">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.apellidos">
							</div>

							<label for="inputName" class="col-md-2 form-label">Nombres *</label>
							<div class="col-md-4">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.nombres">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Cédula *</label>
							<div class="col-md-4">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.cedula">
							</div>

							<label for="inputName" class="col-md-2 form-label">Sexo</label>
							<div class="col-md-4">
								<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="cliente.sexo">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Estado civil</label>
							<div class="col-md-4">
								<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="cliente.estado_civil">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Teléfonos</label>
							<div class="col-md-10">
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Número Oro</th>
										<th>Teléfono</th>
										<th>Tipo</th>
										<th>Origen</th>
										<th>Fecha Ingreso</th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="t in telefono">
										<td>
											<label class="custom-control custom-radio">
												<input type="radio" class="custom-control-input" name="example-radios" value="option1" checked>
												<span class="custom-control-label"></span>
											</label>
										</td>
										<td>
											<div class="input-group">
												<input type="text" class="form-control form-control-sm required digits" v-model="t.telefono" placeholder="Teléfono" v-autoname/>
												<button type="button" class="btn btn-icon btn-info btn-sm" v-on:click="delTelefono(t)">
													<i class="fa fa-phone" aria-hidden="true"></i>
												</button>
											</div>
										</td>
										<td>
											<select class="form-control form-control-sm form-select select2" v-autoname v-model="t.tipo" placeholder="Seleccione una opción" v-autoname>
												<option value="titular">TITULAR</option>
												<option value="familiar">FAMILIAR</option>
												<option value="amigo">AMIGO</option>
												<option value="otros">OTROS</option>
											</select>
										</td>
										<td style="text-transform: uppercase;"><span v-html="t.origen"></span></td>
										<td><span v-html="t.fecha_ingreso"></span></td>
									</tr>
									</tbody>
								</table>
							</div>

							<label for="inputName" class="col-md-2 form-label">Emails</label>
							<div class="col-md-10">
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Email</th>
										<th>Tipo</th>
										<th>Origen</th>
										<th>Fecha Ingreso</th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="c in email">
										<td>
											<input type="text" class="form-control form-control-sm required email" v-model="c.email" placeholder="Email" v-autoname/>
										</td>
										<td>
											<select class="form-control form-select select2" v-autoname v-model="c.tipo" placeholder="Seleccione una opción" v-autoname>
												<option value="titular">TITULAR</option>
												<option value="familiar">FAMILIAR</option>
												<option value="amigo">AMIGO</option>
												<option value="otros">OTROS</option>
											</select>
										</td>
										<td style="text-transform: uppercase;"><span v-html="c.origen"></span></td>
										<td><span v-html="c.fecha_ingreso"></span></td>
									</tr>
									</tbody>
								</table>
							</div>
						</div>

						<div class="mb-0 mt-4 ">
							<div class="col-md-12">
								<input type="hidden" v-model="model.id" id="id" name="id"/>
								<input type="hidden" name="json" id="json"/>
							</div>
						</div>
					</div>
				</div>
			</form>


			<div class="card">
				<div class="card-header">
					<h4 class="card-title">PRODUCTO</h4>
				</div>
				<div class="card-body">
					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Producto</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.producto">
						</div>

						<label for="inputName" class="col-md-2 form-label">Subproducto</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.subproducto">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Agencia</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.agencia">
						</div>

						<label for="inputName" class="col-md-2 form-label">Estado</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.estado">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Estado operación</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.estado_operacion">
						</div>

						<label for="inputName" class="col-md-2 form-label">Tipo proceso</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.tipo_proceso">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Fecha adquisición</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.fecha_adquisicion">
						</div>

						<label for="inputName" class="col-md-2 form-label">Sector</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.sector">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Monto crédito</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.monto_credito">
						</div>

						<label for="inputName" class="col-md-2 form-label">Monto adeudado</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.monto_adeudado">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Monto riesgo</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.monto_riesgo">
						</div>

						<label for="inputName" class="col-md-2 form-label">Días mora</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.dias_mora">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Número de cuotas</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.numero_cuotas">
						</div>

						<label for="inputName" class="col-md-2 form-label">Fecha vencimiento</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.fecha_vencimiento">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Valor cuota</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.valor_cuota">
						</div>

						<label for="inputName" class="col-md-2 form-label">Valor cobrar</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="model.valor_cobrar">
						</div>
					</div>
				</div>
			</div>

			<form method="POST" action="{{ root }}/institucion/guardar" id="form-edit-seguimiento" class="form-horizontal" autocomplete="off" enctype="multipart/form-data">
				<div class="card">
					<div class="card-header">
						<h4 class="card-title">SEGUIMIENTO</h4>
					</div>
					<div class="card-body">
						<h4 class="text-primary">COBRANZA</h4>
						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">NIVEL1</label>
							<div class="col-md-4">
								<select class="form-control form-select select2 required" v-model="model.tipo_gestion" placeholder="Seleccione una opción">
									<option value="ACUERDO DE PAGO DOCUMENTADO">ACUERDO DE PAGO DOCUMENTADO</option>
									<option value="CONTACTO SIN ARREGLO MEDIATO">CONTACTO SIN ARREGLO MEDIATO</option>
									<option value="MENSAJE A TERCERO">MENSAJE A TERCERO</option>
									<option value="NO CONTACTADO MAÑANA / TARDE / NOCHE">NO CONTACTADO MAÑANA / TARDE / NOCHE</option>
									<option value="NOTIFICADO">NOTIFICADO</option>
									<option value="OFRECIMIENTO AL CORTE">OFRECIMIENTO AL CORTE</option>
									<option value="REFINANCIA">REFINANCIA</option>
									<option value="SIN ARREGLO">SIN ARREGLO</option>
								</select>
							</div>

							<label for="inputName" class="col-md-2 form-label">NIVEL2</label>
							<div class="col-md-4">
								<select class="form-control form-select select2 required" v-model="model.tipo_perfil" placeholder="Seleccione una opción">
									<option value="COMPAÑERO DE TRABAJO M">COMPAÑERO DE TRABAJO M</option>
									<option value="EQUIVOCADO-DESCONOCEN NUM M">EQUIVOCADO-DESCONOCEN NUM M</option>
									<option value="CONTESTADORA M">CONTESTADORA M</option>
									<option value="EQUIVOCADO / OTORGAN NÚMERO M">EQUIVOCADO / OTORGAN NÚMERO M</option>
									<option value="TELÉFONO CON AVERIA M">TELÉFONO CON AVERIA M</option>
									<option value="EMPLEADA M">EMPLEADA M</option>
									<option value="FALLECIMIENTO M">FALLECIMIENTO M</option>
									<option value="SUGERENCIA MANTENIMIENTO M">SUGERENCIA MANTENIMIENTO M</option>
									<option value="VIAJE M">VIAJE M</option>
									<option value="CLIENTE CUELGA TELÉFONO M">CLIENTE CUELGA TELÉFONO M</option>
									<option value="DIRECCIÓN NO CORRESPONDE M">DIRECCIÓN NO CORRESPONDE M</option>
									<option value="NO CONTESTA M">NO CONTESTA M</option>
								</select>
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Observaciones</label>
							<div class="col-md-4">
								<textarea rows="3" class="form-control" placeholder="Observaciones" v-autoname></textarea>
							</div>
						</div>

						<div class="mb-0 mt-4 ">
							<div class="col-md-12">
								<input type="hidden" v-model="model.id" id="id" name="id"/>
								<input type="hidden" name="json" id="json"/>

								<button class="btn btn-primary" type="submit" id="btnGuardar" name="btnGuardar">
									<i class="fa fa-save"></i> Guardar
								</button>
							</div>
						</div>

					</div>
				</div>
			</form>

			<div class="card">
				<div class="card-header">
					<h4 class="card-title">APLICATIVO DINERS</h4>
				</div>
				<div class="card-body">
					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">CIUDAD DE GESTIÓN</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.ciudad_gestion">
						</div>

						<label for="inputName" class="col-md-2 form-label">FECHA DE ELABORACIÓN</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" value="{{ "now"|date("Y-m-d H:i:s") }}">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">NEGOCIADO POR</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.negociado_por">
						</div>

						<label for="inputName" class="col-md-2 form-label">NUMERO DE CEDULA SOCIO</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.cedula_socio">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">NOMBRE DEL SOCIO</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.nombre_socio">
						</div>

						<label for="inputName" class="col-md-2 form-label">DIRECCIÓN</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.direccion">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">NÚMEROS DE CONTACTO</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.numero_contactos">
						</div>

						<label for="inputName" class="col-md-2 form-label">MAIL DE CONTACTO</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.mail_contacto">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">CIUDAD DE LA CUENTA</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.ciudad_cuenta">
						</div>

						<label for="inputName" class="col-md-2 form-label">CIUDAD DE LA CUENTA</label>
						<div class="col-md-4">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.zona_cuenta">
						</div>
					</div>

					<div class=" row mb-4">
						<div style="overflow-x:auto;">
							<table style="font-size: 12px;">
								<thead>
								<tr>
									<th style="width: 33.33%; text-align: center; background-color: #0066A8; color: white;">DINERS</th>
									<th style="width: 33.33%; text-align: center; background-color: #404040; color: white;">INTERDIN</th>
									<th style="width: 33.33%; text-align: center; background-color: #E66929; color: white;">DISCOVER</th>
								</tr>
								</thead>
								<tbody>
								<tr>
									<td style="vertical-align: top;">
										<table>
											<tbody>
											<tr>
												<td style="width: 10%;"><strong>CICLO</strong></td>
												<td style="width: 90%; text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.ciclo"></span></td>
											</tr>
											<tr>
												<td><strong>EDAD DE CARTERA</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.edad_cartera"></span></td>
											</tr>
											<tr>
												<td><strong>CODIGO DE CANCELACION</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.codigo_cancelacion"></span></td>
											</tr>
											<tr>
												<td><strong>CODIGO DE BOLETIN</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.codigo_boletin"></span></td>
											</tr>
											<tr>
												<td><strong>DÉBITO AUTOMÁTICO</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.debito_automatico"></span></td>
											</tr>
											<tr style="background-color: #c3c3c3;">
												<td><strong>PROMEDIO DE PAGO</strong></td>
												<td style="text-align: center;"><strong><span v-html="aplicativo_diners_tarjeta_diners.promedio_pago"></span></strong></td>
											</tr>
											<tr>
												<td><strong>SCORE</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.score"></span></td>
											</tr>
											</tbody>
										</table>
										<br/>
										<table>
											<tbody>
											<tr>
												<td style="width: 10%;"><strong>CAMPAÑA</strong></td>
												<td style="width: 90%;" colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.campana"></span></td>
											</tr>
											<tr>
												<td><strong>EJECUTIVO ACTUAL A CARGO DE CUENTA</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.ejecutivo_actual_cuenta"></span></td>
											</tr>
											<tr>
												<td><strong>LUGAR DE TRABAJO</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.lugar_trabajo"></span></td>
											</tr>
											<tr>
												<td><strong>FECHA ÚLTIMA GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.fecha_ultima_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>MOTIVO DE GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.motivo_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>DESCRIPCIÓN DE GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.descripcion_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>OBSERVACIONES DE GESTIÓN</strong></td>
												<td colspan="3">
													<textarea rows="3" v-model="aplicativo_diners_tarjeta_diners.observacion_gestion" class="form-control form-control-sm"
															  placeholder="OBSERVACIONES DE GESTIÓN" v-autoname></textarea>
												</td>
											</tr>
											<tr>
												<td><strong>FECHA DE COMPROMISO</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.fecha_compromiso"></span></td>
												<td style="width: 10%;"><strong>TT/EXIG/PARCIAL</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.tt_exig_parcial"></span></td>
											</tr>
											<tr>
												<td><strong>MOTIVO DE NO PAGO ANTERIOR</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.motivo_no_pago_anterior"></span></td>
											</tr>
											<tr>
												<td><strong>FINANCIAMIENTO VIGENTE</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.financiamiento_vigente"></span></td>
											</tr>
											</tbody>
											<tr>
												<td><strong>N° CUOTAS PENDIENTES</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.numero_cuotas_pendientes"></span></td>
												<td style="width: 10%;"><strong>TT CUOTAS FACT</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.tt_cuotas_fact"></span></td>
											</tr>
											<tr>
												<td><strong>VALOR CUOTAS PENDIENTES</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.valor_cuotas_pendientes"></span></td>
												<td style="width: 10%;"><strong>VALOR DE CUOTA</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.valor_cuota"></span></td>
											</tr>
											<tr style="background-color: #c3c3c3;">
												<td><strong>SEGUNDA REESTRUCTURACIÓN</strong></td>
												<td colspan="3"><strong><span v-html="aplicativo_diners_tarjeta_diners.segunda_restructuracion"></span></strong></td>
											</tr>
											<tr style="background-color: #c3c3c3;">
												<td><strong>TOTAL RIESGO</strong></td>
												<td colspan="3"><strong><span v-html="aplicativo_diners_tarjeta_diners.total_riesgo"></span></strong></td>
											</tr>
										</table>
										<br/>
										<table>
											<tbody>
