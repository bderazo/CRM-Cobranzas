					self.aplicativo_diners_tarjeta_discover.valor_financiar = valor_financiar_diners.toFixed(2);

					//TOTAL INTERES
					var porcentaje_interes_arr = [];
					$.each(self.aplicativo_diners_porcentaje_interes, function (key, value) {
