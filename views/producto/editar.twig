	<script>
		_autonameVueCount = 0;
		Vue.directive('autoname', {
			inserted: function (el, binding, vnode) {
				_autonameVueCount++;
				$(el).prop('name', '_auto' + _autonameVueCount);
			}
		});

		var app = new Vue({
			el: '#app',
			data: {
				cat: {{ catalogos|raw }},
				model: {{ model|raw }},
				telefono: {{ telefono|raw }},
				del_telefono: [],
				email: {{ email|raw }},
				del_email: [],
				cliente: {{ cliente|raw }},
				aplicativo_diners: {{ aplicativo_diners|raw }},
				aplicativo_diners_tarjeta_diners: {{ aplicativo_diners_tarjeta_diners|raw }},
				aplicativo_diners_tarjeta_discover: {{ aplicativo_diners_tarjeta_discover|raw }},
			},
			watch: {},
			computed: {},
			methods: {
				agregarTelefono: function () {
					var self = this;
					var data = {
						telefono: '',
						tipo: 'titular',
						origen: 'SISTEMA',
					};
					self.telefono.push(data);
				},

				delTelefono: function (p) {
					var self = this;
					if (confirm("Está seguro que desea eliminar el telefono: " + p.telefono + "?")) {
						var ix = self.telefono.indexOf(p);
						self.telefono.splice(ix, 1);
						if (p.id)
							self.del_telefono.push(p.id);
					}
				},

				agregarEmail: function () {
					var self = this;
					var data = {
						email: '',
						tipo: 'titular',
						origen: 'SISTEMA',
					};
					self.email.push(data);
				},

				delEmail: function (p) {
					var self = this;
					if (confirm("Está seguro que desea eliminar el email: " + p.email + "?")) {
						var ix = self.email.indexOf(p);
						self.email.splice(ix, 1);
						if (p.id)
							self.del_email.push(p.id);
					}
				},
			},
			mounted: function () {
				var self = this;
				$('#form-edit').validate({
					onfocusout: false,
					onkeyup: false,
					onclick: false,
					submitHandler: function (form) {
						var data = {
							model: self.model,
							telefono: self.telefono,
							del_telefono: self.del_telefono,
							email: self.email,
							del_email: self.del_email,
						};
						$('#json').val(JSON.stringify(data));
						$("#btnGuardar").prop("disabled", true);
						form.submit();
						return false;
					}
				});
			}
		});
	</script>
