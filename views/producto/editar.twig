					//TOTAL FINANCIAMIENTO
					var total_financiamiento = parseFloat(self.aplicativo_diners_tarjeta_discover.valor_financiar) + parseFloat(self.aplicativo_diners_tarjeta_discover.total_intereses);
					self.aplicativo_diners_tarjeta_discover.total_financiamiento_total = total_financiamiento.toFixed(2);

					//VALOR CUOTA MENSUAL
					var cuota_mensual = 0;
