						parseFloat(self.aplicativo_diners_tarjeta_diners.interes_facturar) + parseFloat(self.aplicativo_diners_tarjeta_diners.corrientes_facturar) +
						parseFloat(self.aplicativo_diners_tarjeta_diners.gastos_cobranza) + parseFloat(self.aplicativo_diners_tarjeta_diners.valor_otras_tarjetas) -
						parseFloat(self.aplicativo_diners_tarjeta_diners.abono_total);
					self.aplicativo_diners_tarjeta_diners.valor_financiar = valor_financiar_diners.toFixed(2);

					//TOTAL INTERES
					var porcentaje_interes_arr = [];
					$.each(self.aplicativo_diners_porcentaje_interes, function (key, value) {
						porcentaje_interes_arr[value.meses_plazo] = value.interes;
					});
					var porcentaje_interes = 0.00;
					var meses_plazo = parseInt(self.aplicativo_diners_tarjeta_diners.plazo_financiamiento) + parseInt(self.aplicativo_diners_tarjeta_diners.numero_meses_gracia);
					if (typeof (porcentaje_interes_arr[meses_plazo]) != "undefined" && porcentaje_interes_arr[meses_plazo] !== null) {
						porcentaje_interes = porcentaje_interes_arr[meses_plazo];
					}
					var total_interes = parseFloat(self.aplicativo_diners_tarjeta_diners.valor_financiar) * (porcentaje_interes / 100);
					self.aplicativo_diners_tarjeta_diners.total_intereses = total_interes.toFixed(2);

					//TOTAL FINANCIAMIENTO
					var total_financiamiento = parseFloat(self.aplicativo_diners_tarjeta_diners.valor_financiar) + parseFloat(self.aplicativo_diners_tarjeta_diners.total_intereses);
