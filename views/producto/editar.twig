{% extends 'layout.twig' %}
{% block content %}
	<div id="app">
		<div class="col-md-12 col-xl-12">
			<form method="POST" action="{{ root }}/institucion/guardar" id="form-edit" class="form-horizontal" autocomplete="off" enctype="multipart/form-data">
				<div class="card">
					<div class="card-header">
						<h4 class="card-title">Cliente</h4>
					</div>
					<div class="card-body">

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Apellidos *</label>
							<div class="col-md-4">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.apellidos">
							</div>

							<label for="inputName" class="col-md-2 form-label">Nombres *</label>
							<div class="col-md-4">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.nombres">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Cédula *</label>
							<div class="col-md-4">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.cedula">
							</div>

							<label for="inputName" class="col-md-2 form-label">Sexo</label>
							<div class="col-md-4">
								<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="cliente.sexo">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Estado civil</label>
							<div class="col-md-4">
								<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="cliente.estado_civil">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Teléfonos</label>
							<div class="col-md-10">
								<button class="btn btn-sm btn-secondary" type="button" v-on:click="agregarTelefono">
									<i class="fa fa-plus"></i> Agregar
								</button>
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Número Oro</th>
										<th>Teléfono</th>
										<th>Tipo</th>
										<th>Origen</th>
										<th>Fecha Ingreso</th>
										<th></th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="t in telefono">
										<td>
											<label class="custom-control custom-radio">
												<input type="radio" class="custom-control-input" name="example-radios" value="option1" checked>
												<span class="custom-control-label"></span>
											</label>
										</td>
										<td>
											<div class="input-group">
												<input type="text" class="form-control form-control-sm required digits" v-model="t.telefono" placeholder="Teléfono" v-autoname/>
												<button type="button" class="btn btn-icon btn-info btn-sm" v-on:click="delTelefono(t)">
													<i class="fa fa-phone" aria-hidden="true"></i>
												</button>
											</div>
										</td>
										<td>
											<select class="form-control form-control-sm form-select select2" v-autoname v-model="t.tipo" placeholder="Seleccione una opción" v-autoname>
												<option value="titular">TITULAR</option>
												<option value="familiar">FAMILIAR</option>
												<option value="amigo">AMIGO</option>
												<option value="otros">OTROS</option>
											</select>
										</td>
										<td style="text-transform: uppercase;"><span v-html="t.origen"></span></td>
										<td><span v-html="t.fecha_ingreso"></span></td>
										<td style="text-align: center;">
											<button type="button" class="btn btn-icon btn-danger btn-sm" v-on:click="delTelefono(t)">
												<i class="fe fe-trash"></i>
											</button>

										</td>
									</tr>
									</tbody>
								</table>
							</div>

							<label for="inputName" class="col-md-2 form-label">Emails</label>
							<div class="col-md-10">
								<button class="btn btn-sm btn-secondary" type="button" v-on:click="agregarEmail">
									<i class="fa fa-plus"></i> Agregar
								</button>
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Email</th>
										<th>Tipo</th>
										<th>Origen</th>
										<th>Fecha Ingreso</th>
										<th></th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="c in email">
										<td>
											<input type="text" class="form-control form-control-sm required email" v-model="c.email" placeholder="Email" v-autoname/>
										</td>
										<td>
											<select class="form-control form-select select2" v-autoname v-model="c.tipo" placeholder="Seleccione una opción" v-autoname>
												<option value="titular">TITULAR</option>
												<option value="familiar">FAMILIAR</option>
												<option value="amigo">AMIGO</option>
												<option value="otros">OTROS</option>
											</select>
										</td>
										<td style="text-transform: uppercase;"><span v-html="c.origen"></span></td>
										<td><span v-html="c.fecha_ingreso"></span></td>
										<td style="text-align: center;">
											<button type="button" class="btn btn-icon btn-danger btn-sm" v-on:click="delEmail(c)">
												<i class="fe fe-trash"></i>
											</button>
										</td>
									</tr>
									</tbody>
								</table>
							</div>
						</div>

						<div class="mb-0 mt-4 ">
							<div class="col-md-12">
								<input type="hidden" v-model="model.id" id="id" name="id"/>
								<input type="hidden" name="json" id="json"/>

								<button class="btn btn-primary" type="submit" id="btnGuardar" name="btnGuardar">
									<i class="fa fa-save"></i> Guardar
								</button>
							</div>
						</div>
					</div>
				</div>
			</form>


			<div class="card">
				<div class="card-header">
					<h4 class="card-title">Producto</h4>
				</div>
				<div class="card-body">
					<div class=" row mb-4">
