					self.aplicativo_diners_tarjeta_interdin.total_intereses = total_interes.toFixed(2);

					//TOTAL FINANCIAMIENTO
					var total_financiamiento = parseFloat(self.aplicativo_diners_tarjeta_interdin.valor_financiar) + parseFloat(self.aplicativo_diners_tarjeta_interdin.total_intereses);
					self.aplicativo_diners_tarjeta_interdin.total_financiamiento_total = total_financiamiento.toFixed(2);

					//VALOR CUOTA MENSUAL
					var cuota_mensual = 0;
					if (parseInt(self.aplicativo_diners_tarjeta_interdin.plazo_financiamiento) > 0) {
						cuota_mensual = parseFloat(self.aplicativo_diners_tarjeta_interdin.total_financiamiento_total) / parseInt(self.aplicativo_diners_tarjeta_interdin.plazo_financiamiento);
					}
					self.aplicativo_diners_tarjeta_interdin.valor_cuota_mensual = cuota_mensual.toFixed(2);
				},

				calcularValoresDiscover: function () {
					var self = this;

					//ABONO TOTAL
					var abono_total_diners = parseFloat(self.aplicativo_diners_tarjeta_discover.abono_efectivo_sistema) + parseFloat(self.aplicativo_diners_tarjeta_discover.abono_negociador);
					self.aplicativo_diners_tarjeta_discover.abono_total = abono_total_diners.toFixed(2);

					//VALOR A FINANCIAR
					var valor_financiar_diners = parseFloat(self.aplicativo_diners_tarjeta_discover.deuda_actual) + parseFloat(self.aplicativo_diners_tarjeta_discover.total_precancelacion_diferidos) +
						parseFloat(self.aplicativo_diners_tarjeta_discover.interes_facturar) + parseFloat(self.aplicativo_diners_tarjeta_discover.corrientes_facturar) +
						parseFloat(self.aplicativo_diners_tarjeta_discover.gastos_cobranza) + parseFloat(self.aplicativo_diners_tarjeta_discover.valor_otras_tarjetas) -
