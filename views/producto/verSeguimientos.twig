{% extends 'layout.twig' %}
{% block content %}
	<div id="app">
		<div class="col-md-12 col-xl-12">
			<form method="POST" action="{{ root }}/institucion/guardar" id="form-edit-cliente" class="form-horizontal" autocomplete="off" enctype="multipart/form-data">
				<div class="card">
					<div class="card-header">
						<h4 class="card-title">CLIENTE</h4>
					</div>
					<div class="card-body">
						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Nombres *</label>
							<div class="col-md-4 col-informacion">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.nombres">
							</div>

							<label for="inputName" class="col-md-2 form-label">Cédula *</label>
							<div class="col-md-4 col-informacion">
								<input type="text" readonly class="form-control-plaintext" v-model="cliente.cedula">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Estado civil</label>
							<div class="col-md-4 col-informacion">
								<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="cliente.estado_civil">
							</div>

							<label for="inputName" class="col-md-2 form-label">Sexo</label>
							<div class="col-md-4 col-informacion">
								<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="cliente.sexo">
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Teléfonos</label>
							<div class="col-md-10">
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Número Oro</th>
										<th>Tipo</th>
										<th>Descripción</th>
										<th>Teléfono</th>
										<th>Origen</th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="t in telefono">
										<td>
											<label class="custom-control custom-radio">
												<input type="radio" class="custom-control-input" name="example-radios" value="option1" checked>
												<span class="custom-control-label"></span>
											</label>
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="t.tipo">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="t.descripcion">
										</td>
										<td>
											<div class="input-group">
												<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="t.telefono">
											</div>
										</td>
										<td style="text-transform: uppercase;">
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="t.origen">
										</td>
									</tr>
									</tbody>
								</table>
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Direcciones</label>
							<div class="col-md-10">
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Tipo</th>
										<th>Ciudad</th>
										<th>Dirección</th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="d in direccion">
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="d.tipo">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="d.ciudad">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="d.direccion">
										</td>
									</tr>
									</tbody>
								</table>
							</div>
						</div>

						<div class=" row mb-4">
							<label for="inputName" class="col-md-2 form-label">Referencias</label>
							<div class="col-md-10">
								<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0">
									<thead>
									<tr>
										<th>Tipo</th>
										<th>Descripción</th>
										<th>Nombre</th>
										<th>Teléfono</th>
										<th>Ciudad</th>
										<th>Dirección</th>
									</tr>
									</thead>
									<tbody>
									<tr v-for="r in referencia">
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="r.tipo">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="r.descripcion">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="r.nombre">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="r.telefono">
										</td>
										<td>
											<input type="text" style="text-transform: uppercase;" readonly class="form-control-plaintext" v-model="r.ciudad">
										</td>
										<td>
											<textarea rows="3" v-model="r.direccion" class="form-control-plaintext" readonly placeholder="Dirección" v-autoname></textarea>
										</td>
									</tr>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</form>


			{% if modelArr.institucion_id != 1 %}
			<div class="card">
				<div class="card-header">
					<h4 class="card-title">PRODUCTO</h4>
				</div>
				<div class="card-body">
					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">Código operación</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="model.producto">
						</div>

						<label for="inputName" class="col-md-2 form-label">Estado</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="model.estado">
						</div>
					</div>

					{% set residuo_final = 1 %}
					{% for key, pc in producto_campos %}
						{% if((key % 2) == 0) %}
							<div class=" row mb-4">
						{% endif %}
								<label for="inputName" class="col-md-2 form-label">{{ pc.campo }}</label>
								<div class="col-md-4 col-informacion">
									{{ pc.valor }}
								</div>
						{% if((key % 2) == 1) %}
							</div>
						{% endif %}
						{% set residuo_final = (key % 2) %}
					{% endfor %}
					{% if(residuo_final == 0) %}
						</div>
					{% endif %}
				</div>
			</div>
			{% endif %}

			<div class="card">
				<div class="card-header">
					<h4 class="card-title">SEGUIMIENTO</h4>
				</div>
				<div class="card-body">
					<h4 class="text-primary">COBRANZA</h4>
					<div class="table-responsive">
						<table class="table border text-nowrap text-md-nowrap table-striped table-bordered table-sm mb-0">
							<thead>
							<tr>
								<th>Fecha de seguimiento</th>
								<th>Seguimiento</th>
								<th>Usuario</th>
							</tr>
							</thead>
							<tbody>
							{% for seg in seguimientos %}
							<tr>
								<td>{{ seg.fecha_ingreso }}</td>
								<td>
									{% if seg.nivel1 != '' %}
										<strong>{{ paleta.titulo_nivel1}}: </strong> {{ seg.nivel1 }}<br/>
									{% endif %}
									{% if seg.nivel2 != '' %}
										<strong>{{ paleta.titulo_nivel2}}: </strong> {{ seg.nivel2 }}<br/>
									{% endif %}
									{% if seg.nivel3 != '' %}
										<strong>{{ paleta.titulo_nivel3}}: </strong> {{ seg.nivel3 }}<br/>
									{% endif %}
									{% if seg.nivel4 != '' %}
										<strong>{{ paleta.titulo_nivel4}}: </strong> {{ seg.nivel4 }}<br/>
									{% endif %}
								</td>
								<td>{{ seg.usuario }}</td>
							</tr>
							{% endfor %}
							</tbody>
						</table>
					</div>
				</div>
			</div>

			{% if modelArr.institucion_id == 1 %}
			<div class="card">
				<div class="card-header">
					<h4 class="card-title">APLICATIVO TARJETAS</h4>
				</div>
				<div class="card-body">
					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">PRODUCTO</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="model.producto">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">CIUDAD DE GESTIÓN</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.ciudad_gestion">
						</div>

						<label for="inputName" class="col-md-2 form-label">FECHA DE ELABORACIÓN</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" value="{{ "now"|date("Y-m-d H:i:s") }}">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">NEGOCIADO POR</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.negociado_por">
						</div>

						<label for="inputName" class="col-md-2 form-label">NUMERO DE CEDULA SOCIO</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.cedula_socio">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">NOMBRE DEL SOCIO</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.nombre_socio">
						</div>

						<label for="inputName" class="col-md-2 form-label">DIRECCIÓN</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.direccion">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">SEGURO</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.seguro_desgravamen">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">NÚMEROS DE CONTACTO</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.numero_contactos">
						</div>

						<label for="inputName" class="col-md-2 form-label">MAIL DE CONTACTO</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.mail_contacto">
						</div>
					</div>

					<div class=" row mb-4">
						<label for="inputName" class="col-md-2 form-label">CIUDAD DE LA CUENTA</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.ciudad_cuenta">
						</div>

						<label for="inputName" class="col-md-2 form-label">ZONA DE LA CUENTA</label>
						<div class="col-md-4 col-informacion">
							<input type="text" readonly class="form-control-plaintext" v-model="aplicativo_diners.zona_cuenta">
						</div>
					</div>

					<div class=" row mb-4">
						<div class="table-responsive" style="scrollbar-color: red yellow;">
							<table class="table border text-nowrap text-md-nowrap table-bordered table-sm mb-0 font-size: 10px;">
								<thead>
								<tr>
									<template v-if="aplicativo_diners_tarjeta_diners.id > 0">
										<th style="width: 25%; text-align: center; background-color: #0066A8; color: white;">DINERS</th>
									</template>
									<template v-if="aplicativo_diners_tarjeta_interdin.id > 0">
										<th style="width: 25%; text-align: center; background-color: #404040; color: white;">INTERDIN</th>
									</template>
									<template v-if="aplicativo_diners_tarjeta_discover.id > 0">
										<th style="width: 25%; text-align: center; background-color: #E66929; color: white;">DISCOVER</th>
									</template>
									<template v-if="aplicativo_diners_tarjeta_mastercard.id > 0">
										<th style="width: 25%; text-align: center; background-color: #A4B706; color: white;">MASTERCARD</th>
									</template>
								</tr>
								</thead>
								<tbody>
								<tr>
									<template v-if="aplicativo_diners_tarjeta_diners.id > 0">
									<td style="vertical-align: top;">
										<table>
											<tbody>
											<tr>
												<td style="width: 10%;"><strong>CICLO</strong></td>
												<td style="width: 90%; text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.ciclo"></span></td>
											</tr>
											<tr>
												<td><strong>EDAD DE CARTERA</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.edad_cartera"></span></td>
											</tr>
											<tr>
												<td><strong>CODIGO DE CANCELACION</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.codigo_cancelacion"></span></td>
											</tr>
											<tr>
												<td><strong>CODIGO DE BOLETIN</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.codigo_boletin"></span></td>
											</tr>
											<tr>
												<td><strong>DÉBITO AUTOMÁTICO</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.debito_automatico"></span></td>
											</tr>
											<tr>
												<td><strong>SCORE</strong></td>
												<td style="text-align: center;"><span v-html="aplicativo_diners_tarjeta_diners.score"></span></td>
											</tr>
											</tbody>
										</table>
										<br/>
										<table>
											<tbody>
											<tr>
												<td style="width: 10%;"><strong>CAMPAÑA</strong></td>
												<td style="width: 90%;" colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.campana"></span></td>
											</tr>
											<tr>
												<td><strong>EJECUTIVO ACTUAL A CARGO DE CUENTA</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.ejecutivo_actual_cuenta"></span></td>
											</tr>
											<tr>
												<td><strong>LUGAR DE TRABAJO</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.lugar_trabajo"></span></td>
											</tr>
											<tr>
												<td><strong>FECHA ÚLTIMA GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.fecha_ultima_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>MOTIVO DE GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.motivo_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>DESCRIPCIÓN DE GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.descripcion_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>OBSERVACIONES DE GESTIÓN</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.observacion_gestion"></span></td>
											</tr>
											<tr>
												<td><strong>FECHA DE COMPROMISO</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.fecha_compromiso"></span></td>
												<td style="width: 10%;"><strong>No FIN HISTÓRICOS</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.tt_exig_parcial"></span></td>
											</tr>
											<tr>
												<td><strong>OFERTA VALOR</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.oferta_valor"></span></td>
											</tr>
											<tr>
												<td><strong>REFINANCIACIONES ANTERIORES</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.refinanciaciones_anteriores"></span></td>
											</tr>
											<tr>
												<td><strong>CARDIA</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.cardia"></span></td>
											</tr>
											<tr>
												<td><strong>MOTIVO DE NO PAGO ANTERIOR</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.motivo_no_pago_anterior"></span></td>
											</tr>
											<tr>
												<td><strong>FINANCIAMIENTO VIGENTE</strong></td>
												<td colspan="3"><span v-html="aplicativo_diners_tarjeta_diners.financiamiento_vigente"></span></td>
											</tr>
											</tbody>
											<tr>
												<td><strong>N° CUOTAS PENDIENTES</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.numero_cuotas_pendientes"></span></td>
												<td style="width: 10%;"><strong>TT CUOTAS FACT</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.tt_cuotas_fact"></span></td>
											</tr>
											<tr>
												<td><strong>VALOR CUOTAS PENDIENTES</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.valor_cuotas_pendientes"></span></td>
												<td style="width: 10%;"><strong>VALOR DE CUOTA</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.valor_cuota"></span></td>
											</tr>
											<tr style="background-color: #c3c3c3;">
												<td><strong>SEGUNDA REESTRUCTURACIÓN</strong></td>
												<td colspan="3"><strong><span v-html="aplicativo_diners_tarjeta_diners.segunda_restructuracion"></span></strong></td>
											</tr>
											<tr style="background-color: #c3c3c3;">
												<td><strong>TOTAL RIESGO</strong></td>
												<td colspan="3"><strong><span v-html="aplicativo_diners_tarjeta_diners.total_riesgo"></span></strong></td>
											</tr>
										</table>
										<br/>
										<table>
											<tbody>
											<tr>
												<td colspan="2" style="text-align: center; background-color: #0066A8; color: white;"><strong>SALDOS FACTURADOS</strong></td>
											</tr>
											<tr>
												<td style="width: 60%;"><strong>SALDO A 90 Y MAS 90 DÍAS</strong></td>
												<td style="width: 40%;"><span v-html="aplicativo_diners_tarjeta_diners.saldo_90_facturado"></span></td>
											</tr>
											<tr>
												<td><strong>SALDO A 60 DIAS</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.saldo_60_facturado"></span></td>
											</tr>
											<tr>
												<td><strong>SALDO A 30 DIAS</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.saldo_30_facturado"></span></td>
											</tr>
											<tr>
												<td><strong>SALDO ACTUALES</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.saldo_actual_facturado"></span></td>
											</tr>
											<tr>
												<td style="text-decoration: underline;"><strong>DEUDA ACTUAL</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.deuda_actual"></span></td>
											</tr>
											<tr>
												<td style="text-decoration: underline;"><strong> &nbsp; </strong></td>
												<td></td>
											</tr>
											</tbody>
										</table>
										<br/>
										<table>
											<tbody>
											<tr>
												<td style="width: 60%;"><strong>INTERESES FACTURADOS</strong></td>
												<td style="width: 40%; background-color: #c3c3c3;"><span v-html="aplicativo_diners_tarjeta_diners.interes_facturado"></span></td>
											</tr>
											<tr>
												<td><strong>NUMERO DE DIFERIDOS FACTURADOS</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.numero_diferidos_facturados"></span></td>
											</tr>
											<tr>
												<td><strong>TOTAL VALOR PRE CANCELACION DIFERIDOS</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.total_precancelacion_diferidos"></span></td>
											</tr>
											<tr>
												<td><strong>ESPECIALIDAD VENTA VEHICULOS</strong></td>
												<td><span v-html="aplicativo_diners_tarjeta_diners.especialidad_venta_vehiculos"></span></td>
											</tr>
											</tbody>
										</table>
