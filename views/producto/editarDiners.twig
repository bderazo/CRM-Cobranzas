                verificarCampos: function (tarjeta = '') {
                    var self = this;
                    if(tarjeta == '') {
                        var data = {
                            nivel_1_id: self.seguimiento.nivel_1_id,
                            nivel_2_id: self.seguimiento.nivel_2_id,
                            nivel_3_id: self.seguimiento.nivel_3_id,
                            nivel_4_id: self.seguimiento.nivel_4_id,
