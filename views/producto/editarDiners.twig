                $('#form-edit-seguimiento').on('keyup keypress', function(e) {
                    var keyCode = e.keyCode || e.which;
                    if (keyCode === 13) {
                        e.preventDefault();
                        return false;
                    }
                });

                var maxchars = 238;
                $('#observaciones').keyup(function () {
                    var tlength = $(this).val().length;
                    $(this).val($(this).val().substring(0, maxchars));
                    var tlength = $(this).val().length;
