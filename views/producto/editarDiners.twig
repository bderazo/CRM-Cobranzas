            watch: {},
            computed: {},
            methods: {
                verificarSeguimientosTarjeta: function () {
                    var self = this;
                    if((self.seguimiento.nivel_1_id == '1855') && (self.bandera_unificar_deuda == 'no')){
                        $(".seguimiento_general").hide();
                        if(self.aplicativo_diners_tarjeta_diners.length != 0){
                            if(self.aplicativo_diners_tarjeta_diners.motivo_cierre != 'PAGADA') {
                                $("#div_seguimiento_diners").show();
                            }
                        }
                        if(self.aplicativo_diners_tarjeta_interdin.length != 0){
                            if(self.aplicativo_diners_tarjeta_interdin.motivo_cierre != 'PAGADA') {
                                $("#div_seguimiento_interdin").show();
                            }
                        }
                        if(self.aplicativo_diners_tarjeta_discover.length != 0){
                            if(self.aplicativo_diners_tarjeta_discover.motivo_cierre != 'PAGADA') {
                                $("#div_seguimiento_discover").show();
                            }
                        }
                        if(self.aplicativo_diners_tarjeta_mastercard.length != 0){
                            if(self.aplicativo_diners_tarjeta_mastercard.motivo_cierre != 'PAGADA') {
                                $("#div_seguimiento_mastercard").show();
                            }
                        }
                    }else{
                        $(".seguimiento_general").show();
                        $("#div_seguimiento_diners").hide();
                        $("#div_seguimiento_interdin").hide();
                        $("#div_seguimiento_discover").hide();
                        $("#div_seguimiento_mastercard").hide();
                    }

                    self.verificarCampos('diners');
                    self.verificarCampos('interdin');
                    self.verificarCampos('discover');
                    self.verificarCampos('mastercard');
                },

                cargarNivel2: function (tarjeta = '') {
                    var self = this;
                    if(tarjeta == '') {
                        var data = {
                            nivel_1_id: self.seguimiento.nivel_1_id,
                        };
                        self.cat.paleta_nivel_2 = [];
                        self.cat.paleta_nivel_3 = [];
                        self.seguimiento.nivel_2_id = 0;
                        self.seguimiento.nivel_3_id = 0;
                        self.verificarCampos(tarjeta);
                        $.post('{{ root }}/paleta/cargarNivel2', data, function (res) {
                            self.cat.paleta_nivel_2 = res;
