{% extends 'layout.twig' %}
{% block content %}
    <div id="app">
        <form method="POST" action="{{ root }}/producto/guardarSeguimientoDiners" id="form-edit-seguimiento"
              class="form-horizontal" autocomplete="off" enctype="multipart/form-data">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">CLIENTE</h4>
                </div>
                <div class="card-body">
                    <div class=" row mb-4">
                        <label for="inputName" class="col-md-2 form-label">Nombres</label>
                        <div class="col-md-4 col-informacion">
                            <h3 style="font-weight: bold;"><span class="form-control-plaintext" v-html="cliente.nombres"></span></h3>
                        </div>

                        <label for="inputName" class="col-md-2 form-label">Cédula</label>
                        <div class="col-md-4 col-informacion">
                            <h3 style="font-weight: bold;"><span class="form-control-plaintext" v-html="cliente.cedula"></span></h3>
                        </div>
                    </div>

                    <div class=" row mb-4">
                        <label for="inputName" class="col-md-2 form-label">Gestionar</label>
                        <div class="col-md-4 col-informacion">
                            <template v-if="cliente.gestionar != 'no'">
                                <h3 style="font-weight: bold; text-transform: uppercase;"><span style="color: green;" class="form-control-plaintext" v-html="cliente.gestionar"></span></h3>
                            </template>
                            <template v-else>
                                <h3 style="font-weight: bold; text-transform: uppercase;"><span style="color: red;" class="form-control-plaintext" v-html="cliente.gestionar"></span></h3>
                            </template>
                        </div>

                        <label for="inputName" class="col-md-2 form-label">Experiencia crediticia</label>
                        <div class="col-md-4 col-informacion">
                            <h3 style="font-weight: bold;"><span class="form-control-plaintext" v-html="cliente.experiencia_crediticia"></span></h3>
                        </div>
                    </div>

                    <div class=" row mb-4">
                        <label for="inputName" class="col-md-2 form-label">Productos</label>
                        <div class="col-md-10">
                            <table class="table border table-striped table-bordered table-sm mb-0" style="font-size: 12px;">
                                <thead>
                                <tr>
                                    <th style="font-size: 12px;">MARCA</th>
                                    <th style="font-size: 12px;">CICLO</th>
                                    <th style="font-size: 12px;">INTERÉS FACTURADO</th>
                                    <th style="font-size: 12px;">DEUDA ACTUAL</th>
                                    <th style="font-size: 12px;">TOTAL RIESGO</th>
                                    <th style="font-size: 12px;">EDAD CARTERA</th>
                                    <th style="font-size: 12px;">DIFERIDOS FACTURADOS</th>
                                    <th style="font-size: 12px;">SALDO ACTUAL FACTURADO</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr v-for="a in asignacion">
                                    <td style="text-align: center;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.marca"></span>
                                    </td>
                                    <td style="text-align: center;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.ciclo"></span>
                                    </td>
                                    <td style="text-align: right;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.aplicativo.interes_facturado"></span>
                                    </td>
                                    <td style="text-align: right;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.aplicativo.deuda_actual"></span>
                                    </td>
                                    <td style="text-align: right;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.aplicativo.total_riesgo"></span>
                                    </td>
                                    <td style="text-align: right;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.aplicativo.edad_cartera"></span>
                                    </td>
                                    <td style="text-align: right;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.aplicativo.numero_diferidos_facturados"></span>
                                    </td>
                                    <td style="text-align: right;">
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="a.aplicativo.saldo_actual_facturado"></span>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>

                    <div class=" row mb-4">
                        <label for="inputName" class="col-md-2 form-label">Teléfonos</label>
                        <div class="col-md-10">
                            <table>
                                <tr>
                                    <td style="background-color: #CCFFCC; width: 20px; color: white; border: 1px solid white;"></td>
                                    <td style="border: 1px solid white;">Número ORO</td>
                                </tr>
                            </table>
                            <table class="table border table-bordered table-sm mb-0">
                                <thead>
                                <tr>
                                    <th style="width: 10%;"></th>
                                    <th style="width: 30%;">Marcación / Teléfono</th>
                                    <th style="width: 30%;">Descripción</th>
                                    <th style="width: 30%;">Origen</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr v-for="t in telefono">
                                    <td style="text-align: center;">
                                        <label class="custom-control custom-radio">
                                            <input type="radio" class="custom-control-input required" name="example-radios" v-model="seguimiento.telefono_id" :value="t.id">
                                            <template v-if="t.bandera == 1">
                                                <span style="background-color: #CCFFCC;" class="custom-control-label"></span>
                                            </template>
                                            <template v-else>
                                                <span class="custom-control-label"></span>
                                            </template>
                                        </label>
                                    </td>
                                    <td style="text-align: center;">
                                        <template v-if="t.telefono_link_wh == ''">
                                            <template v-if="t.bandera == 1">
                                                <span style="background-color: #CCFFCC;" class="custom-control-label" v-html="t.telefono"></span>
                                            </template>
                                            <template v-else>
                                                <span class="custom-control-label" v-html="t.telefono"></span>
                                            </template>
                                        </template>
                                        <template v-else>
                                            <template v-if="t.bandera == 1">
                                                <span style="background-color: #CCFFCC;" class="custom-control-label" v-html="t.telefono"></span>
                                                <a :href="t.telefono_link_wh" target="_blank" title="LINK WHATSAPP">
                                                    <img src="{{ root }}/img/whatsapp_logo.png" alt="LINK WHATSAPP">
                                                </a>
                                            </template>
                                            <template v-else>
                                                <span class="custom-control-label" v-html="t.telefono"></span>
                                                <a :href="t.telefono_link_wh" target="_blank" title="LINK WHATSAPP">
                                                   <img src="{{ root }}/img/whatsapp_logo.png" alt="LINK WHATSAPP">
                                                </a>
                                            </template>
                                        </template>
                                    </td>
                                    <td>
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="t.descripcion"></span>
                                    </td>
                                    <td>
                                        <span style="text-transform: uppercase;" class="form-control-plaintext" v-html="t.origen"></span>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class=" row mb-4">
                        <label for="inputName" class="col-md-2 form-label">Direcciones</label>
                        <div class="col-md-10">
                            <table>
                                <tr>
                                    <td style="background-color: #CCFFCC; width: 20px; color: white; border: 1px solid white;"></td>
                                    <td style="border: 1px solid white;">Dirección ORO</td>
                                </tr>
                            </table>
                            <table class="table border table-bordered table-sm mb-0">
                                <thead>
                                <tr>
                                    <th>Tipo</th>
                                    <th>Ciudad</th>
                                    <th>Dirección</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr v-for="d in direccion">
                                    <td>
                                        <span style="text-transform: uppercase;" class="form-control-plaintext"
